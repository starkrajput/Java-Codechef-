int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[n];
	    int height=0;
	    int sum=0;
	    for(int i=0;i<n;i++){
	        a[i]=i+1;
	        sum+=a[i];
	        if(sum<=n){
	            height++;
	        }
	        else{
	            break;
	        }
	    }
	    cout<<height<<endl;
	}
	return 0;
}
